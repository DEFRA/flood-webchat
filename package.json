{
  "name": "@defra/flood-webchat",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "src/server/index.js",
  "browser": "src/client/index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test",
    "dev": "npm run compile-templates && node -r dotenv/config node_modules/.bin/webpack server --config demo/webpack.config.mjs",
    "unit-test": "jest",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:scss -- --fix",
    "lint:js": "standard",
    "lint:scss": "stylelint \"**/*.scss\"",
    "postinstall": "npm run compile-templates",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') { throw e } }\"",
    "compile-templates": "mkdir -p dist && nunjucks-precompile templates > ./dist/templates.js"
  },
  "standard": {
    "ignore": [
      "demo/dist"
    ],
    "env": [
      "jest"
    ]
  },
  "keywords": [],
  "author": "DEFRA",
  "license": "OGL-UK-3.0",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.6.4",
    "babel-loader": "^9.1.3",
    "core-js": "^2.6.11",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-cache": "^5.1.2",
    "node-sass": "^9.0.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "standard": "^17.1.0",
    "stylelint": "^15.10.3",
    "stylelint-config-gds": "^1.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "govuk-frontend": "^3.14.0"
  },
  "dependencies": {
    "@nice-devone/nice-cxone-chat-web-sdk": "1.3.0",
    "axios": "^0.24.0",
    "nunjucks": "^3.2.4"
  }
}
